            action="#"
            onSubmit={handleSubmit}
            style={{
              backgroundColor: '#FFFFFF',
              display: 'flex',
              alignItems: 'center',
              justifyContent: 'center',
              flexDirection: 'column',
              padding: '0 50px',
              height: '100%',
              textAlign: 'center',
            }}
          >
            <h1>Create Account</h1>
            <input type="text" placeholder="First name" style={{ backgroundColor: '#eee', border: 'none', padding: '12px 15px', margin: '8px 0', width: '100%' }} />
            <input type="text" placeholder="Last name" style={{ backgroundColor: '#eee', border: 'none', padding: '12px 15px', margin: '8px 0', width: '100%' }} />
            <input type="email" placeholder="Email" style={{ backgroundColor: '#eee', border: 'none', padding: '12px 15px', margin: '8px 0', width: '100%' }} />
            <input type="number" placeholder="Age" style={{ backgroundColor: '#eee', border: 'none', padding: '12px 15px', margin: '8px 0', width: '100%' }} />
            <input type="password" placeholder="Password" id="pswd1" style={{ backgroundColor: '#eee', border: 'none', padding: '12px 15px', margin: '8px 0', width: '100%' }} />
            <input type="password" placeholder="Confirm password" id="pswd2" style={{ backgroundColor: '#eee', border: 'none', padding: '12px 15px', margin: '8px 0', width: '100%' }} />
            <button type="button" onClick={matchPassword} style={{ borderRadius: '20px', border: '1px solid rgb(20 114 177)', backgroundColor: 'rgb(20 114 177)', color: '#FFFFFF', fontSize: '12px', fontWeight: 'bold', padding: '12px 45px', letterSpacing: '1px', textTransform: 'uppercase', transition: 'transform 80ms ease-in' }}>
              Sign Up
            </button>
          </form>
        </div>
        <div
          className="form-container sign-in-container"
          style={{
            position: 'absolute',
            left: 0,
            width: '50%',
            zIndex: 2,
            transition: 'all 0.6s ease-in-out',
          }}
        >
          <form action="#" style={{ backgroundColor: '#FFFFFF', display: 'flex', alignItems: 'center', justifyContent: 'center', flexDirection: 'column', padding: '0 50px', height: '100%', textAlign: 'center' }}>
